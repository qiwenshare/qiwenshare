        <ffmpeg.version>6.0-1.5.9</ffmpeg.version>
        <minio.version>8.5.6</minio.version>
        <qiniu.version>7.14.0</qiniu.version>
        <hutool.version>5.8.22</hutool.version>
        <springdoc-openapi.version>1.7.0</springdoc-openapi.version>
        <okhttp.version>5.0.0-alpha.3</okhttp.version>
        <aliyun-sdk.version>4.6.4</aliyun-sdk.version>
        <sevenzipjbinding.version>16.02-2.01</sevenzipjbinding.version>
        <jpinyin.version>1.1.8</jpinyin.version>
        <jaudiotagger.version>2.0.3</jaudiotagger.version>
        <htmlunit.version>2.70.0</htmlunit.version>
        <oshi-core.version>6.4.7</oshi-core.version>
        <elasticsearch.version>8.10.4</elasticsearch.version>
